{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "type": "prompt",
      "segments": [
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#1e3a8a",
          "properties": {
            "style": "short"
          }
        }
      ]
    },
    {
      "alignment": "right",
      "type": "prompt",
      "segments": [
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffd700",
          "background": "#2d3748",
          "template": "{{ .UserName }}@{{ .HostName }}"
        },
        {
          "type": "time",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#2d3748",
          "properties": {
            "time_format": "03:04 PM"
          }
        }
      ]
    },
    {
      "alignment": "left",
      "type": "prompt",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "template": " âž¤"
        }
      ]
    }
  ],
  "final_space": true,
  "console_title_template": "{{ .Shell }} {{ .UserName }}@{{ .HostName }}",
  "version": 3
}